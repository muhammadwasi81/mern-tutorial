    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.9",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
